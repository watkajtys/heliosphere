{
	"name": "heliolens",
	"version": "2.0.0",
	"description": "Solar timelapse video generator from NASA satellite data - Built by Vibes",
	"private": true,
	"type": "module",
	"scripts": {
		"start": "node vps_production_optimized.js",
		"build": "tsc",
		"test": "node vps_2day_test.js",
		"test:optimized": "node vps_optimized_test.js",
