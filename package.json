		"typescript": "^5.5.2"
	},
	"dependencies": {
		"@google-cloud/storage": "^7.16.0",
		"archiver": "^7.0.1",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"p-queue": "^8.0.0",
		"pm2": "^5.3.0",
		"sharp": "^0.33.4",
		"ws": "^8.17.0"
